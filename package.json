    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "speed-measure-webpack-plugin": "^1.3.1",
    "superagent": "^4.1.0",
    "supertest": "^4.0.2",
    "ts-loader": "^5.4.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-deep-scope-plugin": "^1.6.0",
    "webpack-merge": "^4.2.1"
  },
