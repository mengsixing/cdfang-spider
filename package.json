{
  "name": "cdfang-spider",
  "version": "2.4.3",
  "description": "成都房协网爬虫，定时爬取最新房源，可视化数据分析。",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "lint": "eslint --ext tsx,ts,js src build __tests__ --fix",
    "dev:client": "cross-env BABEL_ENV=production webpack-dev-server --open",
    "dev:server": "cross-env NODE_ENV=development gulp & cross-env NODE_ENV=development supervisor -i ./dist/client/ -w ./dist/ ./dist/app.js & wait",
    "prebuild": "npm run lint && rimraf ./dist",
    "build": "cross-env NODE_ENV=production gulp & cross-env BABEL_ENV=production NODE_ENV=production webpack",
    "analysis": "cross-env BABEL_ENV=production NODE_ENV=production BUILD_ENV=analysis webpack",
    "test": "jest --no-cache --colors --coverage --forceExit --detectOpenHandles",
    "coverage": "codecov",
    "commit": "npx git-cz"
  },
  "husky": {
    "hooks": {
      "pre-push": "eslint --fix --ext jsx,js src build __tests__"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhlben/cdfang-spider.git"
  },
  "author": "yhlben",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yhlben/cdfang-spider/issues"
  },
  "homepage": "https://github.com/yhlben/cdfang-spider#readme",
  "dependencies": {
    "@antv/data-set": "^0.10.2",
    "antd": "^3.15.1",
    "apollo-boost": "^0.3.1",
    "apollo-server-koa": "^2.4.8",
    "autoprefixer": "^9.5.0",
    "babel-polyfill": "^6.26.0",
    "bizcharts": "^3.4.5",
    "cheerio": "^1.0.0-rc.2",
    "cssnano": "^4.1.10",
    "dayjs": "^1.8.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^3.2.0",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.11",
    "log4js": "^4.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "mongoose": "^5.4.19",
    "node-schedule": "^1.3.2",
